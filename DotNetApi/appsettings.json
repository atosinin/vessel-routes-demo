{
  "JWTSettings": {
    "Issuer": "localhost",
    "Audience": "localhost",
    "AccessTokenLifeSpanInMinutes": 15,
    "RefreshTokenLifeSpanInDays": 7
  },
  "FrontendSettings": {
    "Url": "https://localhost:4200",
    "ErrorPageUrl": "https://localhost:4200/error",
    "LoginUrl": "https://localhost:4200/login",
    "ChangePasswordUrl": "https://localhost:4200/changePassword"
  },
  "EmailSettings": {
    "SmtpHost": "smtp.office365.com",
    "SmtpPort": 587
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Error"
      }
    },
    "WriteTo": [ { "Name": "Console" } ],
    "Enrich": [ "FromLogContext" ],
    "Properties": {
      "Application": "WhateverApp DEV"
    }
  }
}
